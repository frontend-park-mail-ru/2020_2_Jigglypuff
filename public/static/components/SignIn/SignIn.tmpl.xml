<fest:template xmlns:fest="http://fest.mail.ru" context-name="signInForm">
    <style>
        <fest:insert src="./SignIn.css"/>
    </style>
    <form method="POST" class="wrapper__form" id="signInForm">
        <h2>Вход</h2>
        <main>
            <input type="email" placeholder="Логин"/>
            <input type="password" placeholder="Пароль"/>
            <a class="pass" href="#">Восстановить пароль</a>
            <button type="submit">Войти</button>
        </main>
        <button>Регистрация</button>
    </form>
</fest:template>

